# UCEHub Environment Configuration
# Copy this file to .env and update with your values

# API URL - Backend endpoint
# For local development: http://localhost:3001
# For production (ALB): http://your-alb-dns-name.us-east-1.elb.amazonaws.com
# For production (relative paths): leave empty or set to empty string
VITE_API_URL=http://localhost:3001

# Grafana Monitoring URL
# For local development: http://localhost:3000
# For production (ALB): http://your-alb-dns-name.us-east-1.elb.amazonaws.com/grafana
# Or direct IP: http://grafana-private-ip:3000
VITE_GRAFANA_URL=http://localhost:3000

# Teams Configuration (optional)
VITE_TEAMS_APP_ID=
VITE_TEAMS_TENANT_ID=
